{
  "manifest_version": 3,
  "name": "New Tab Dashboard",
  "description": "Replace your new tab page with a highly customizable dashboard.",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "assets/icons/icon-72x72.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["bookmarks", "topSites", "favicon"]
}
